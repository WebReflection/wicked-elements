{
  "name": "wicked-elements",
  "version": "2.0.2",
  "description": "Components for the DOM as you've never seen before",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "unpkg": "min.js",
  "scripts": {
    "build": "npm run cjs && npm run rollup:new && npm run rollup:babel && npm run min && npm run size && npm run test",
    "cjs": "ascjs --no-default esm cjs",
    "rollup:new": "rollup --config rollup/new.config.js",
    "rollup:babel": "rollup --config rollup/babel.config.js",
    "min": "uglifyjs index.js --support-ie8 --comments=/^!/ -c -m -o min.js",
    "size": "cat min.js | gzip -9 | wc -c && cat min.js | brotli | wc -c && cat new.js | gzip -9 | wc -c && cat new.js | brotli | wc -c",
    "test": "echo \"$(tput smso)$(tput bold) Please check the test folder $(tput sgr0)\" && http-server ."
  },
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "keywords": [
    "dom",
    "custom",
    "elements",
    "define",
    "behavior"
  ],
  "dependencies": {
    "@ungap/element-matches": "^0.2.0",
    "@ungap/node-contains": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@ungap/degap": "^0.2.0",
    "ascjs": "^4.0.0",
    "http-server": "^0.12.3",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "uglify-js": "^3.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/wicked-elements.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/wicked-elements/issues"
  },
  "homepage": "https://github.com/WebReflection/wicked-elements#readme",
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "default": "./cjs/index.js"
  }
}
